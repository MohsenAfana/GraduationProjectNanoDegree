<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ibrahimelhout.graduationprojectnanodegree">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:icon">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginAcitivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".Utils.MotherActivity">
        </activity>
        <activity android:name=".ProductsListActivity">
        </activity>
        <activity android:name=".ProductsActivity">
        </activity>
        <activity android:name=".ProductDetailsActivity">
        </activity>
    </application>

</manifest>